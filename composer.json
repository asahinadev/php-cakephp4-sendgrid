{
	"name" : "asahinadev/cakephp-sendgrid",
	"description" : "SendGrid plugin for CakePHP",
	"type" : "cakephp-plugin",
	"license" : "MIT",
	"require" : {
		"php" : ">=7.2",
		"cakephp/cakephp" : "~4.1.0",
		"sendgrid/sendgrid" : "~7.9"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^8.0"
	},
	"autoload" : {
		"psr-4" : {
			"App\\SendGrid\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"App\\SendGrid\\Test\\" : "tests/",
			"Cake\\Test\\" : "vendor/cakephp/cakephp/tests/"
		}
	},
	"homepage" : "https://github.com/asahinadev/php-cakephp4-sendgrid",
	"keywords" : [
		"sendgrid"
	]
}